<if cond="%{id}"><SetVisible name="insertxtmsm_carrierdetail" Visible="0"/><SetVisible name="modifyxtmsm_carrierdetail" Visible="1"/></if>
</OnClick>
</ListCtrl>
</Ctrls></GroupBox>