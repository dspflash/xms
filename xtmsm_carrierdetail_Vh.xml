<GroupBox Name="GBDVxtmsm_carrierdetail" cols="xs12 sm12 md12 lg12" visible="0"><Ctrls>
<ListCtrl Name="xtmsm_carrierdetailLst" ToolTip="ttp_xtmsm_carrierdetail" Size="70,20" visible="0" loadedclick="1">
