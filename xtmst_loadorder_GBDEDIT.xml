	<DbDetails>
<Index>carrierid</Index>
<DbTable>tms.xtmsm_carrier</DbTable>
<Data>CarrierName,CarrierID</Data>
<Where>CarrierID='%{VehicleRealTimeID[CarrierID]}'</Where>
<queryFormat/></DbDetails></TextBox>
</Ctrls></GroupBox>
</Ctrls></GroupBox>